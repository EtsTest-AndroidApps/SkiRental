<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.ModificareDateActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:text="Weight"
        app:layout_constraintBottom_toBottomOf="@+id/editTextGreutateM"
        app:layout_constraintEnd_toStartOf="@+id/editTextGreutateM"
        app:layout_constraintTop_toTopOf="@+id/editTextGreutateM" />

    <TextView
        android:id="@+id/textViewYourData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="188dp"
        android:text="Your data"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextEmailM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textViewYourData"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textViewYourData"
        app:layout_constraintTop_toBottomOf="@+id/textViewYourData" />

    <EditText
        android:id="@+id/editTextNumeM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/editTextEmailM"
        app:layout_constraintStart_toStartOf="@+id/editTextEmailM"
        app:layout_constraintTop_toBottomOf="@+id/editTextEmailM" />

    <EditText
        android:id="@+id/editTextParolaM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/editTextNumeM"
        app:layout_constraintStart_toStartOf="@+id/editTextNumeM"
        app:layout_constraintTop_toBottomOf="@+id/editTextNumeM" />

    <EditText
        android:id="@+id/editTextGreutateM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/editTextParolaM"
        app:layout_constraintStart_toStartOf="@+id/editTextParolaM"
        app:layout_constraintTop_toBottomOf="@+id/editTextParolaM" />

    <EditText
        android:id="@+id/editTextVarstaM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/editTextGreutateM"
        app:layout_constraintStart_toStartOf="@+id/editTextGreutateM"
        app:layout_constraintTop_toBottomOf="@+id/editTextGreutateM" />

    <EditText
        android:id="@+id/editTextMarimePiciorM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/editTextVarstaM"
        app:layout_constraintStart_toStartOf="@+id/editTextVarstaM"
        app:layout_constraintTop_toBottomOf="@+id/editTextVarstaM" />

    <EditText
        android:id="@+id/editTextNivelM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/editTextMarimePiciorM"
        app:layout_constraintStart_toStartOf="@+id/editTextMarimePiciorM"
        app:layout_constraintTop_toBottomOf="@+id/editTextMarimePiciorM" />

    <EditText
        android:id="@+id/editTextInaltimeM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="@+id/editTextNivelM"
        app:layout_constraintStart_toStartOf="@+id/editTextNivelM"
        app:layout_constraintTop_toBottomOf="@+id/editTextNivelM" />

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroupModificare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:singleSelection="true"
        app:layout_constraintEnd_toEndOf="@+id/editTextInaltimeM"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editTextInaltimeM"
        app:layout_constraintTop_toBottomOf="@+id/editTextInaltimeM">

        <com.google.android.material.chip.Chip
            android:id="@+id/chipFata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checkable="true"
            android:text="F" />

        <com.google.android.material.chip.Chip
            android:id="@+id/chipBaiat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:checkable="true"
            android:text="M" />
    </com.google.android.material.chip.ChipGroup>

    <Button
        android:id="@+id/buttonModificaDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="249dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="48dp"
        android:text="Modify"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:text="Email"
        app:layout_constraintBottom_toBottomOf="@+id/editTextEmailM"
        app:layout_constraintEnd_toStartOf="@+id/editTextEmailM"
        app:layout_constraintTop_toTopOf="@+id/editTextEmailM"
        app:layout_constraintVertical_bias="0.538" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="44dp"
        android:text="Name"
        app:layout_constraintBottom_toBottomOf="@+id/editTextNumeM"
        app:layout_constraintEnd_toStartOf="@+id/editTextNumeM"
        app:layout_constraintTop_toTopOf="@+id/editTextNumeM"
        app:layout_constraintVertical_bias="0.384" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="Password"
        app:layout_constraintBottom_toBottomOf="@+id/editTextParolaM"
        app:layout_constraintEnd_toStartOf="@+id/editTextParolaM"
        app:layout_constraintTop_toTopOf="@+id/editTextParolaM" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:text="Age"
        app:layout_constraintBottom_toBottomOf="@+id/editTextVarstaM"
        app:layout_constraintEnd_toStartOf="@+id/editTextVarstaM"
        app:layout_constraintTop_toTopOf="@+id/editTextVarstaM"
        app:layout_constraintVertical_bias="0.384" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:text="Foot size"
        app:layout_constraintBottom_toBottomOf="@+id/editTextMarimePiciorM"
        app:layout_constraintEnd_toStartOf="@+id/editTextMarimePiciorM"
        app:layout_constraintTop_toTopOf="@+id/editTextMarimePiciorM"
        app:layout_constraintVertical_bias="0.346" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="65dp"
        android:layout_height="42dp"
        android:layout_marginEnd="16dp"
        android:text="Difficulty level"
        app:layout_constraintBottom_toBottomOf="@+id/editTextNivelM"
        app:layout_constraintEnd_toStartOf="@+id/editTextNivelM"
        app:layout_constraintTop_toTopOf="@+id/editTextNivelM"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:text="Height"
        app:layout_constraintBottom_toBottomOf="@+id/editTextInaltimeM"
        app:layout_constraintEnd_toStartOf="@+id/editTextInaltimeM"
        app:layout_constraintTop_toTopOf="@+id/editTextInaltimeM"
        app:layout_constraintVertical_bias="0.576" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:text="Sex"
        app:layout_constraintBottom_toBottomOf="@+id/chipGroupModificare"
        app:layout_constraintEnd_toStartOf="@+id/chipGroupModificare"
        app:layout_constraintTop_toTopOf="@+id/chipGroupModificare"
        app:layout_constraintVertical_bias="0.551" />
</androidx.constraintlayout.widget.ConstraintLayout>